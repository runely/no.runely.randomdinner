{
  "name": "no.runely.randomdinner",
  "version": "1.0.0",
  "main": "app.js",
  "devDependencies": {
    "homey": "2.8.0",
    "xo": "0.36.1"
  },
  "scripts": {
    "test": "xo"
  },
  "xo": {
    "space": 2,
    "semicolon": true,
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "object-curly-spacing": "off",
      "eol-last": "off",
      "capitalized-comments": "off"
    }
  }
}
